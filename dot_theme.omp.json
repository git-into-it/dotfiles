{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "python",
          "style": "plain",
          "template": "[<#FFD43B> </> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}]"
        },
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " \ue5fa ",
            "yarn_icon": " \ue6a7"
          },
          "style": "plain",
          "template": "[<#2C8EBB>\ue718</> {{ if .PackageManagerIcon }}<#CB3837>{{ .PackageManagerIcon }}</> {{ end }}{{ .Full }}]",
          "type": "node"
        },
        {
          "style": "plain",
          "template": "[<#00ADD8></> {{ .Full }}]",
          "type": "go"
        },
        {
          "type": "rust",
          "style": "plain",
          "template": "[<#D34516></> {{ .Full }}]"
        },
        {
          "type": "java",
          "style": "plain",
          "template": "[<#007396></> {{ .Full }}]"
        },
        {
          "type": "lua",
          "style": "plain",
          "template": "[<#000080> </>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}]"
        },
        {
          "properties": {
            "branch_icon": " \ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "plain",
          "template": "[<#A6E3A1>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}]",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{ if .Env.IN_NIX_SHELL }}\uf313 {{ else }}{{.Icon}}{{ end }} ",
          "type": "os"
        },
        {
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": " -> ",
            "home_icon": "\ueb06"
          },
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "plain",
          "template": "[{{ .FormattedMs }}]",
          "type": "executiontime"
        },
        {
          "foreground": "#00ff15",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ else }}#00ff15{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " {{ if eq .Code 0 }}✔{{ else }}✘{{ end }}",
          "type": "status"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " \u276f "
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "\ue285 "
  },
  "version": 3
}
